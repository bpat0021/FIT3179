{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "Locations of bike racks in the city of Ballarat",
  "width": 325,
  "height": 400,
  "background": "white",
  "title": {"text":"Melbounre Bike Rack Locations", "color": "black", "fontSize":18, "fontWeight":100},

  "layer": [
    {
      "data": {
        "url": "./output.json",
        "format": {"type": "topojson", "feature": "suburb-10-vic"}
      },
      "mark": {
        "type": "geoshape",
        "fill":"lightgray",
        "stroke": "black"
      },
      "projection": {"type":"mercator"}
    
    },
    {
        "data":{"url": "./Data/bikeracks_melb.csv"},
        "mark": {"type":"point", "size": 50, "color":"red", "opacity":0.5},
        "encoding": {
            "longitude":{"field":"lon", "type": "quantitative"}, 
            "latitude":{"field":"lat", "type": "quantitative"}
        }
    }
    ]
}